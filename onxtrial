import cv2

# Load ONNX model
net = cv2.dnn.readNetFromONNX('best.onnx')

# Load image
frame = cv2.imread('test.jpg')
blob = cv2.dnn.blobFromImage(frame, 1/255.0, (640, 640), swapRB=True, crop=False)
net.setInput(blob)

# Forward pass
outputs = net.forward()

